        "bug_ids": [
            "CSCvu06343"
        ],
        "cves": [
            "CVE-2020-3431"
        ],
        "cvrfUrl": "https://tools.cisco.com/security/center/contentxml/CiscoSecurityAdvisory/cisco-sa-sa-rv-routers-xss-K7Z5U6q3/cvrf/cisco-sa-sa-rv-routers-xss-K7Z5U6q3_cvrf.xml",
        "cvss_base_score": "6.1",
